                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="StabServoMax1(Am)" />
                <EditText
                    android:id="@+id/StabServoMax1"
                    android:layout_width="100px"
                    android:layout_height="wrap_content"
                    android:ems="10"
                    android:inputType="number" />
