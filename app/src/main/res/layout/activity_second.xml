                    android:id="@+id/PITCH_RANGEVALUE"
                    android:layout_width="100px"
                    android:layout_height="wrap_content"
                    android:ems="10"
                    android:inputType="number" />
            </LinearLayout>



            </LinearLayout>


                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="35dp"
                    android:orientation="horizontal">
                    <TextView
                        android:id="@+id/textExp"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="exp" />
                    <EditText
                        android:id="@+id/manageExp"
                        android:layout_width="100px"
                        android:layout_height="wrap_content"
                        android:ems="10"
                        android:inputType="number" />
                </LinearLayout>
        </LinearLayout>

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="horizontal">
                <TextView
                    android:id="@+id/textSERVO1_TYPEMOVE"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="servo1_TM" />
                <Spinner
                    android:id="@+id/SERVO1_TYPEMOVE"
                    android:layout_width="200dp"
                    android:layout_height="30dp"
                    android:entries="@array/motorTypeMove"/>
            </LinearLayout>


            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="horizontal">
                <TextView
                    android:id="@+id/textSERVO2_TYPEMOVE"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="servo1_TM" />
                <Spinner
                    android:id="@+id/SERVO2_TYPEMOVE"
                    android:layout_width="200dp"
                    android:layout_height="30dp"
                    android:entries="@array/motorTypeMove"/>
            </LinearLayout>


            <Button
                android:id="@+id/btnApply"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Apply" />


        </LinearLayout>



    </LinearLayout>

    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal">

        <TextView
            android:id="@+id/toChange"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="servo1_TM" />

        <SeekBar
            android:id="@+id/seekBar"
            android:max="255"
            android:layout_width="300dp"
            android:layout_height="wrap_content" />

        <TextView
            android:id="@+id/toChangeIndic"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="255" />

        <SeekBar
            android:id="@+id/sb_Motor"
            android:max="255"
            android:layout_width="200dp"
            android:layout_height="wrap_content" />


        <Button
            android:id="@+id/btnReset"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="reset" />

