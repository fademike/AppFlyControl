                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="horizontal">
                <TextView
                    android:id="@+id/textStabKoeffCh1"
                    android:layout_width="match_parent"
                    android:layout_height="35dp"
                    android:text="StabKoeffCh1(pD)" />
                <EditText
                    android:id="@+id/StabKoeffCh1"
                    android:layout_width="100px"
                    android:layout_height="wrap_content"
                    android:ems="10"
                    android:inputType="number" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="horizontal">
                <TextView
                    android:id="@+id/textStabKoeffCh2"
                    android:layout_width="match_parent"
                    android:layout_height="35dp"
                    android:text="StabKoeffCh2(mux)" />
                <EditText
                    android:id="@+id/StabKoeffCh2"
                    android:layout_width="100px"
                    android:layout_height="wrap_content"
                    android:ems="10"
                    android:inputType="number" />
            </LinearLayout>

