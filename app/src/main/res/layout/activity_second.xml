                android:layout_height="35dp"
                android:orientation="horizontal">
                <TextView
                    android:id="@+id/textStabServoMax2"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="StabServoMax2(yP)" />
                <EditText
                    android:id="@+id/StabServoMax2"
                    android:layout_width="100px"
                    android:layout_height="wrap_content"
                    android:ems="10"
                    android:inputType="number" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="35dp"
                android:orientation="horizontal">
                <TextView
                    android:id="@+id/textmath_K_angle"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="math_K_angle(yI)" />
                <EditText
                    android:id="@+id/math_K_angle"
                    android:layout_width="100px"
                    android:layout_height="wrap_content"
                    android:ems="10"
                    android:inputType="number" />
            </LinearLayout>

            <LinearLayout
