                    android:id="@+id/textStabYaw"
                    android:layout_width="match_parent"
                    android:layout_height="35dp"
                    android:text="StabYaw(rD)" />
                <EditText
                    android:id="@+id/StabYaw"
                    android:layout_width="100px"
                    android:layout_height="wrap_content"
                    android:ems="10"
                    android:inputType="number" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="35dp"
                android:orientation="horizontal">
                <TextView
                    android:id="@+id/textStabAngelPitch"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="StabAngelPitch(CL)" />
                <EditText
                    android:id="@+id/StabAngelPitch"
                    android:layout_width="100px"
                    android:layout_height="wrap_content"
                    android:ems="10"
                    android:inputType="number" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="35dp"
                android:orientation="horizontal">
                <TextView
                    android:id="@+id/textStabAngelRoll"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="StabAngelRoll" />
