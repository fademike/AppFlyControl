                    android:layout_width="100px"
                    android:layout_height="wrap_content"
                    android:ems="10"
                    android:inputType="number" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="35dp"
                android:orientation="horizontal">
                <TextView
                    android:id="@+id/textROLL_RANGEVALUE"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="roll_range" />
                <EditText
                    android:id="@+id/ROLL_RANGEVALUE"
                    android:layout_width="100px"
                    android:layout_height="wrap_content"
                    android:ems="10"
                    android:inputType="number" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="35dp"
                android:orientation="horizontal">
                <TextView
                    android:id="@+id/textPITCH_INVERSION"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="pitch_inv" />
                <EditText
                    android:id="@+id/PITCH_INVERSION"
                    android:layout_width="100px"
                    android:layout_height="wrap_content"
                    android:ems="10"
                    android:inputType="number" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="35dp"
                android:orientation="horizontal">
                <TextView
                    android:id="@+id/textPITCH_RANGEVALUE"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="pitch_range" />
